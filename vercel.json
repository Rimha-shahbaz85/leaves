{
    "version": 2,
    "name": "leaf-disease-detection",
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/disease-detection-file",
            "dest": "/api/index.py"
        },
        {
            "src": "/api",
            "dest": "/api/index.py"
        },
        {
            "src": "/",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ]
}